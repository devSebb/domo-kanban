<div class="mx-10">
  <h1 class="font-bold text-2xl my-5">My Organizations</h1>
  <ul class="">
    <% @organizations.each do |organization| %>
      <li class="my-2">
        <%= link_to organization.name, organization_path(organization), class: "text-gray-600 font-semibold text-lg hover:text-blue-800 " %>
      </li>
    <% end %>
  </ul>
</div>
